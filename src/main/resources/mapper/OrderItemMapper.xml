<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.coek.mapper.OrderItemMapper">

    <insert id="createOrderItem">
        INSERT INTO t_order_item(

                oid,
                pid,
                title,
                image,
                price,
                num,
                created_user,
                created_time,
                modified_user,
                modified_time

        )VALUES (

                #{oid},
                #{pid},
                #{title},
                #{image},
                #{price},
                #{num},
                #{createdUser},
                #{createdTime},
                #{modifiedUser},
                #{modifiedTime}
                        )
    </insert>
</mapper>
